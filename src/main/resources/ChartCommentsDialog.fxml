<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>

<VBox prefHeight="471.0" prefWidth="716.0" xmlns:fx="http://javafx.com/fxml" fx:controller="maunaloa.controllers.impl.MongoDBCommentsController">
  <children>
    <HBox prefHeight="100.0" prefWidth="200.0">
      <children>
        <TextArea fx:id="txaArea" prefHeight="-1.0" prefWidth="-1.0" wrapText="true" HBox.hgrow="ALWAYS" />
      </children>
    </HBox>
    <HBox prefHeight="-1.0" prefWidth="-1.0">
      <children>
        <Button fx:id="btnAddComment" mnemonicParsing="false" text="Add Comment">
          <HBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" fx:id="x1" />
          </HBox.margin>
        </Button>
        <Button fx:id="btnDeleteComment" mnemonicParsing="false" text="Delete Comment" HBox.margin="$x1" />
        <Button fx:id="btnClose" mnemonicParsing="false" text="Close" HBox.margin="$x1" />
      </children>
    </HBox>
    <HBox prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
        <TreeView fx:id="treeComments" prefHeight="-1.0" prefWidth="-1.0" HBox.hgrow="ALWAYS" />
      </children>
    </HBox>
  </children>
</VBox>
